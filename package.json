{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "esto sera divertido",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multy-entry-points/webpack.config.js",
    "build:loader": "webpack --config ./style-css-loader/webpack.config.js",
    "build:plugin": "webpack --config ./plugins/webpack.config.js",
    "build:dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertJohanson/Curso-Webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@Johanson_aj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbertJohanson/Curso-Webpack/issues"
  },
  "homepage": "https://github.com/AlbertJohanson/Curso-Webpack#readme",
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "5.0.1",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "1.3.1",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "4.40.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.11.0"
  }
}
